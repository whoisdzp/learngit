Class Person{
	private int age;
	private String name;
	public person(String name, int age){
		this.name = name;
		this.age = age;
	}
	public void show(){
		System.out.println("name: " + name + "\n" + "age: " + age);
	}
	
	public void setName(String name){
		this.name = name;
	}
	public String getName(){
		return this.name;
	}
}
